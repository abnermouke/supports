{
    "name": "abnermouke/supports",
    "description": "Supports是Abnermouke整理的PHP开源工具包，安装非常简单，因为它是一个标准的Composer包，这意味着只要使用的是PHP都可以正常使用它。",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Abnermouke\\Supports\\": "src/"
        },
        "files": [
            "helpers/data.php"
        ]
    },
    "authors": [
        {
            "name": "abnermouke",
            "email": "abnermouke@outlook.com"
        }
    ],
    "require": {
        "ext-openssl": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "guzzlehttp/guzzle": "*",
        "intervention/image": "*",
        "simplesoftwareio/simple-qrcode": "*",
        "qiniu/php-sdk": "*",
        "symfony/css-selector": "*",
        "symfony/dom-crawler": "*",
        "w7corp/easywechat": "*",
        "yansongda/pay": "*",
        "overtrue/easy-sms": "*",
        "illuminate/events": "*",
        "illuminate/container": "*",
        "illuminate/database": "*"
    },
    "scripts": {
        "post-update-cmd": [
            "Abnermouke\\Supports\\AbnermoukeSupportsEvents::updated"
        ],
        "post-install-cmd": [
            "Abnermouke\\Supports\\AbnermoukeSupportsEvents::installed"
        ],
        "post-autoload-dump": [
            "Abnermouke\\Supports\\AbnermoukeSupportsEvents::autoload"
        ]
    }
}
