{
    "name": "abnermouke/supports",
    "description": "Supports是Abnermouke整理的PHP开源工具包，安装非常简单，因为它是一个标准的Composer包，这意味着只要使用的是PHP都可以正常使用它。",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Abnermouke\\Supports\\": "src/"
        },
        "files": [
            "helpers/data.php"
        ]
    },
    "authors": [
        {
            "name": "abnermouke",
            "email": "abnermouke@outlook.com"
        }
    ],
    "require": {
        "ext-openssl": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "guzzlehttp/guzzle": "*",
        "intervention/image": "*",
        "simplesoftwareio/simple-qrcode": "*",
        "qiniu/php-sdk": "*",
        "symfony/css-selector": "*",
        "symfony/dom-crawler": "*",
        "yansongda/pay": "*",
        "overtrue/easy-sms": "*",
        "illuminate/events": "*",
        "illuminate/database": "*"
    },
    "scripts": {
        "post-install-cmd": [
            "php -r \"file_exists('config/orm_database.php') || copy('vendor/abnermouke/supports/config/orm_database.php', 'config/orm_database.php');\"",
            "php -r \"file_exists('public/static/avatars/1.png') || copy('vendor/abnermouke/supports/data/avatars', 'public/static/avatars');\""
        ]
    }
}
